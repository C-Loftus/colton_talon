# Left button
key(keypad_divide:down):    
   user.left_pedal()
key(keypad_divide:up):     
    user.left_pedal_up() 

# Right button (kpmult)
key(keypad_multiply:down):
    user.right_pedal()
key(keypad_multiply:up):  
    user.right_pedal_up() 

# Center button (kpplus)
key(keypad_minus:down):    
    user.center_pedal()
key(keypad_minus:up):       
    user.center_pedal_up()

